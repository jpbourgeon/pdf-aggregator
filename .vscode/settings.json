{
  "eslint.autoFixOnSave": true,
  "editor.fontFamily": "'DejaVu Sans Mono', Consolas, 'Courier New', monospace",
  "editor.fontSize": 12,
  "editor.minimap.enabled": false,
  "editor.rulers": [120],
  "editor.tabSize": 2,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 120,
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/node_modules": false,
    "renderer/out": false,
    "renderer/.next": false,
    ".babelrc": false,
    ".leveldb": false,
    ".vscode": false,
    ".eslintignore": false,
    ".eslintrc": false,
    ".gitignore": false,
    ".gitattributes": false,
    "package.json": false,
    "package-lock.json": false,
    "license.txt": false,
    "dist": false,
    "renderer/next.config.js": false,
    "db.json": false
  },
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "Cobalt2",
  "workbench.iconTheme": "vs-seti",
  "workbench.startupEditor": "none"
}