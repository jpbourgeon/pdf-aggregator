{
  "eslint.autoFixOnSave": true,
  "editor.fontFamily": "'DejaVu Sans Mono', Consolas, 'Courier New', monospace",
  "editor.fontSize": 12,
  "editor.minimap.enabled": false,
  "editor.rulers": [120],
  "editor.tabSize": 2,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 120,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "renderer/out": true,
    "renderer/.next": true,
    ".babelrc": true,
    ".leveldb": true,
    ".vscode": true,
    ".eslintignore": true,
    ".eslintrc": true,
    ".gitignore": true,
    ".gitattributes": true,
    "package.json": true,
    "package-lock.json": true,
    "license.txt": true,
    "renderer/next.config.js": true,
    ".circleci": true
  },
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "Cobalt2",
  "workbench.iconTheme": "vs-seti",
  "workbench.startupEditor": "none",
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "LineCount.showStatusBarItem": false,
  "LineCount.output": {
    "txt": false,
    "json": false,
    "csv": false,
    "md": false,
    "outdir": ""
  },
  "LineCount.excludes": [
    ".vscode/**",
    ".git/**",
    "dist/**",
    "node_module/**",
    "renderer/.next/**",
    "renderer/out/**",
    "db.json",
    "package-lock.json"
  ],
  "LineCount.includes": [
      "**/*"
  ]
}